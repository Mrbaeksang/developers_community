{
  "name": "developers_community",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:slow": "next dev",
    "dev:3001": "next dev -p 3001 --turbo",
    "dev:3002": "next dev -p 3002 --turbo",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "test": "npm run lint && npm run type-check",
    "verify": "npm run lint:fix && npm run format && npm run type-check",
    "prisma:validate": "prisma validate && prisma format --check",
    "check:all": "npm run prisma:validate && npm run type-check && npm run lint",
    "db:generate": "prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "dotenv -e .env -- tsx prisma/seed.ts",
    "db:test": "dotenv -e .env -- tsx test-db.ts",
    "sync-redis": "dotenv -e .env.local -- tsx scripts/sync-redis-views.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.2.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/nextjs": "^10.0.0",
    "@tanstack/react-query": "^5.83.0",
    "@tiptap/extension-image": "^3.0.9",
    "@tiptap/extension-placeholder": "^3.0.9",
    "@tiptap/react": "^3.0.9",
    "@tiptap/starter-kit": "^3.0.9",
    "@types/marked": "^5.0.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.1.1",
    "@vercel/kv": "^3.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.9",
    "ioredis": "^5.7.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.526.0",
    "marked": "^16.1.1",
    "next": "15.4.4",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "prisma": "^6.12.0",
    "rate-limiter-flexible": "^7.2.0",
    "react": "19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.61.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "zod": "^4.0.10",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.9.0",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "micromatch": "^4.0.8",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
